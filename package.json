{
  "name": "youtube-stats-chainlink-adapter",
  "version": "1.0.0",
  "description": "Chainlink External Adapter for YouTube video statistics with Supabase state management",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.js --network sepolia",
    "add-consumer": "hardhat run scripts/add-consumer.js --network sepolia",
    "test": "hardhat test",
    "oracle:status": "node_modules/.bin/hardhat run scripts/check-status.js --network arbitrumSepolia",
    "oracle:setup": "node_modules/.bin/hardhat run scripts/set-source-code.js --network arbitrumSepolia",
    "oracle:request": "node_modules/.bin/hardhat run scripts/request-views.js --network arbitrumSepolia",
    "oracle:monitor": "node_modules/.bin/hardhat run scripts/monitor-events.js --network arbitrumSepolia",
    "oracle:add-consumer": "node_modules/.bin/hardhat run scripts/add-consumer-functions.js --network arbitrumSepolia 0xD66544E49c7407AcdE0a577BFB176f950a18DAAA",
    "web": "npx serve public -p 3000"
  },
  "keywords": [
    "chainlink",
    "external-adapter",
    "youtube",
    "supabase"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@chainlink/contracts": "^1.5.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "hardhat": "^2.19.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
